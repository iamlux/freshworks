{
  "name": "pickurtrails-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5",
    "react-scroll-to": "^2.0.5"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-react",
    "start-react": "react-scripts start",
    "build-css": "node-sass-chokidar src/assets/scss/ -o src/assets/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/scss/ -o src/assets/css/ --watch --recursive",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
